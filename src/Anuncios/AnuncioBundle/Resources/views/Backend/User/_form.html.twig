{{ form_errors(form) }}
<div class="row">
    <div class="col-md-6">
		{{ form_row(form.name) }}
		{{ form_row(form.surname) }}
    </div>
    <div class="col-md-6">
    	{{ form_row(form.username) }}
		{{ form_row(form.email) }}
		{{ form_row(form.isjurado) }}
    </div>
</div>
{{ form_rest(form) }}
